<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.minus.prac2.dao.ProductDao">
    <select id="getProdById" parameterType="long" resultType="org.minus.prac2.dto.ProductDto">
        select prod_id as id, prod_name as prodName from products where prod_id = #{id}
    </select>
</mapper>
